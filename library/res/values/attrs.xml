<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MTWidget">
        <attr name="mtUserGrowthViewStyle" format="reference" />
        <attr name="mtMenuViewStyle" format="reference" />
        <attr name="mtAlphabetViewStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="UserGrowthView">
        <attr name="drawable_0" format="reference" />
        <attr name="drawable_1" format="reference" />
        <attr name="drawable_2" format="reference" />
        <attr name="drawable_3" format="reference" />
        <attr name="drawable_4" format="reference" />
        <attr name="drawable_5" format="reference" />
